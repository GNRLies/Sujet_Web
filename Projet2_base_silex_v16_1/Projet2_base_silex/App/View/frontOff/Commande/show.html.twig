{% extends "layout.html.twig" %}
{% block contenu %}
	<div class="row">

		<table>
			<caption>Commandes</caption>
			<thead>
			<tr><th>id</th>><th>prix</th><th>date_achat</th><th>etat</th>
			</tr>
			</thead>
			<tbody>
			{% for commande in data if data is not empty %}

				<tr>
					<td>{{commande.id}}</td><td>{{commande.prix}}</td><td>{{commande.date_achat}}</td><td>
						{{commande.libelle}}</td>
				</tr>
			{% endfor %}
			<tbody>
		</table>
	</div>


{% endblock %}

